{
    "workbench.colorTheme":"Dark (Molokai)",
    "editor": {
        "fontFamily": "'Roboto Mono', 'monospace', monospace",
        "fontSize": 15,
        "fontLigatures": false,
        "insertSpaces": true,
        "inlineSuggest.enabled": true,
        "minimap.enabled": false,
        "renderWhitespace": "all"
    },
    "diffEditor": {
        "ignoreTrimWhitespace": false
    },
    "files.associations": {
        "*.cs": "csharp",
        "*.dart": "dart",
        "*.spec": "rpm-spec"
    },
    "git.mergeEditor": false,
    "git.autofetch": true,
    "git.confirmSync": true,
    "dart": {
        "openDevTools": true,
        "editor.formatOnType": true,
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "javascript": {
        "updateImportsOnFileMove.enabled": true
    },
    "typescript": {
        "updateImportsOnFileMove.enabled": true
    },
    "mesonbuild": {
        "configureOnOpen": true
    },
    "omnisharp": {
        "analyzeOpenDocumentsOnly": true,
        "enableImportCompletion": true
    },
    "ruby": {
        "useBundler": true,
        "useLanguageServer": true,
        "locate": {
            "exclude": "{**/@(test|spec|tmp|.*),**/@(test|spec|tmp|.*)/**,**/*_spec.rb}",
            "include": "**/*.rb"
        }
    },
    "solargraph": {
        "diagnostics": true,
        "externalServer": {
            "host": "localhost",
            "port": 7658
        }
    },
    "rust-analyzer": {
        "server.path": "/usr/bin/rls"
    },
    "settingsSync.ignoredExtensions": [],
    "editor.inlineSuggest.enabled": true,
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "aws.profile": "profile:personal",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "rust-analyzer.server.path": "/usr/bin/rls",
    "rust-analyzer.checkOnSave.enable": false,
    "rust-analyzer.completion.privateEditable.enable": true,
    
}